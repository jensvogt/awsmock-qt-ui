{
  "release-type": "simple",
  "packages": {
    ".": {
      "release-type": "simple",
      "bump-minor-pre-major": true,
      "version-file": "CMakeLists.txt",
      "extra-files": [
        {
          "type": "regex",
          "path": "CMakeLists.txt",
          "search": "project\\(awsmock-qt-ui VERSION ([0-9]+\\.[0-9]+\\.[0-9]+)",
          "replace": "project(awsmock-qt-ui VERSION ${version}"
        }
      ]
    }
  }
}